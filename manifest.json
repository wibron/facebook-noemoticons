{
  "name": "Facebook icon-remover",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Removes the ugly emoticons in the timeline.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*"],
      "js": ["main.js"]
    }
  ],
  "permissions": [
    "tabs"
  ]
}